<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cnnc.springbootmybatis.dao.UserDao">
    <cache />
    <select id="get" resultType="com.alibaba.fastjson.JSONObject" useCache="true" flushCache="false">
        select * from t_user where id=#{id}
    </select>

    <insert id="save" parameterType="com.cnnc.springbootmybatis.domain.User">
        insert into t_user(name, age) values(#{name}, #{age})
    </insert>
</mapper>



